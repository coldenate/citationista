		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.15.1"
	}
