		"@types/jest": "^29.5.14",
		"@types/node": "^18.0.3",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.2",
		"autoprefixer": "^10.4.7",
		"bestzip": "^2.2.1",
		"concurrently": "^7.2.2",
		"copy-webpack-plugin": "^11.0.0",
