{
	"private": true,
	"name": "remnote-plugin-template-react",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"check-types": "tsc",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --ext .ts,.tsx",
		"format": "prettier --check .",
		"prepare": "husky install",
		"start:dev": "cross-env NODE_ENV=development webpack-dev-server --color --progress --no-open",
		"dev": "concurrently \"npm run start:dev\" \"npm run test:watch\"",
		"devSync": "ts-node src/devSyncCli.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"build": "npx remnote-plugin validate && shx rm -rf dist && cross-env NODE_ENV=production webpack --color --progress && shx cp README.md dist && cd dist && bestzip ../PluginZip.zip ./*",
		"generate-snippet": "ts-node scripts/generateCssSnippet.ts",
		"docs": "typedoc",
		"deps": "madge --image docs/deps.svg src/modules"
	},
	"dependencies": {
		"@remnote/plugin-sdk": "0.0.44",
		"citation-js": "^0.7.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"zotero-api-client": "^0.43.1"
	},
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
		"@types/jest": "^29.5.14",
		"@types/node": "^18.0.3",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.2",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"autoprefixer": "^10.4.7",
		"bestzip": "^2.2.1",
		"concurrently": "^7.2.2",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"esbuild-loader": "^4.3.0",
		"eslint": "^8.50.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.0",
		"jest": "^29.7.0",
		"lint-staged": "^15.0.0",
		"mini-css-extract-plugin": "^2.6.1",
		"patch-package": "^8.0.0",
		"postcss": "^8.4.31",
		"postcss-import": "^14.1.0",
		"postcss-loader": "^7.0.0",
		"prettier": "^2.8.8",
		"react-refresh": "^0.14.0",
		"shx": "^0.3.4",
		"style-loader": "^3.3.1",
		"tailwindcss": "^3.1.5",
		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.15.1",
		"typedoc": "^0.25.5",
		"madge": "^6.0.0"
	},
	"engines": {
		"node": "^18"
	}
}
